<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.erikbuto.workoutprogram" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Home.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppThemeNoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Manage.ManageProgramActivity"
            android:label="@string/title_activity_manage_program" >
        </activity>
        <activity
            android:name=".Manage.ManageExerciseActivity"
            android:label="@string/title_activity_manage_exercise" >
        </activity>
    </application>

</manifest>
